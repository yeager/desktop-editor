#!/usr/bin/env python3
import sys
import os
sys.path.insert(0, '/usr/lib/python3/dist-packages')

try:
    from desktop_editor.main import main
    main()
except ImportError:
    try:
        from desktop_editor.app import main
        main()
    except ImportError:
        print("Error: Cannot find main module for desktop-editor", file=sys.stderr)
        sys.exit(1)
